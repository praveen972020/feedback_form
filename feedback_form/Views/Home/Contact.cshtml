        Academic: ['I', 'II', 'III', 'IV', 'V', 'VI', 'VII', 'VIII'],
        A: ['I', 'II', 'III', 'IV', 'V', 'VI', 'VII', 'VIII'],
    }



    $('#select_course').change(function () {
        var course = $(this).val();;
        // alert(course);
        // var course = 'All';
        var sem = s_sem[course];
        $('#select_sem').empty();


        if (sem) {
            // $('#select_sem').append($('<option>').text('Please Choose An Option'));
