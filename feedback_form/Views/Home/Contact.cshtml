                <div class="tab-pane fade" id="profile">
                    <div class="row">
                        <!-- left column -->
                        <div class="col-md-12">
                            <!-- general form elements -->
                            <div class="card card-success">
                                <div class="card-header">
                                    <h3 class="card-title">Student Feedback</h3>
                                </div>
                                <!-- /.card-header -->
                                <!-- form start -->
                                <form role="form" name="myform">
                                    <div class="card-body">
                                        <div class="row">
                                            <div class="col-md-4">
                                                <select name="select_course1" id="select_course1" ng-model="select_course1" class="form-select" required>
                                                    <option disabled selected value>Course</option>
                                                  
                                                    <option value="MCA">MCA</option>
                                                    <option value="BCA">BCA</option>
                                                    <option value="MBA">MBA</option>
                                                    <option value="B.TECH">B.Tech</option>
                                                </select>
                                            </div>
                                            <div class="col-md-4">
                                                <select name="Semester" id="Semester" ng-model="Semester" class="form-select" required>
                                                    <option disabled selected value>Semester</option>
                                                  
                                                    <option value="I">I</option>
                                                    <option value="II">II</option>
                                                    <option value="III">III</option>
                                                    <option value="IV">IV</option>
                                                    <option value="V">V</option>
                                                    <option value="VI">VI</option>
                                                </select>
                                            </div>
                                            <div class="col-md-4">

                                                <button type="submit" class="btn btn-success float-right form-control" title="Please select the field" @*ng-disabled="myform.$invalid"*@ ng-click="fetchRecordctrl()">Search</button>
                                            </div>
                                        </div>
                                    </div>
                                </form>

                            </div>

                            <div class="" id="fetch_table" style="display:none;">
                                <table class="table table-striped">

                                    <thead>
                                        <tr>
                                            <th style="text-align:left;">Sno</th>
                                            <th style="text-align:center;">Course</th>
                                            <th style="text-align:center;">Semester</th>
                                            <th>Date From</th>
                                            <th>Date To</th>
                                            <th>Total Student</th>
                                            <th>Total Submisson</th>
                                            <th>Pending</th>
                                            <th style="text-align:right">Question</th>
                                        </tr>
                                    </thead>


                                    <tbody>
                                        <tr >
                                            <td style="text-align:left;">{{$index+1}}</td>
                                            <td style="text-align:center;">{{AllRecord[0].course}}</td>
                                            <td style="text-align: center;">{{AllRecord[0].Semester}}</td>
                                            <td style="padding:16px;">{{AllRecord[0].datefrom}}</td>
                                            <td style="padding:16px;">{{AllRecord[0].dateto}}</td>
                                            <td style="text-align: center;">
                                                <a href="#"
                                                   id="myBtn"
                                                   data-toggle="modalx"
                                                   ng-click="total();"
                                                   data-target="#modalx">
                                                    <u><b>{{AllRecord[0].Total_stu}}</b></u>
                                                </a>
                                            </td>
                                            <td style="text-align:center" ;>
                                                <a href="#"
                                                   data-toggle="modalsub"
                                                   ng-click="submission();"
                                                   data-target="#modalsub">
                                                    <u><b>{{AllRecord[0].Total_submission}}</b></u>
                                                </a>
                                            </td>
                                            <td style="text-align:center;">

                                                <a href="#"
                                                   data-toggle="modalnot"
                                                   ng-click="notsub();"
                                                   data-target="#modalnot">
                                                    <u><b>{{AllRecord[0].Remain_stu}}</b></u>
                                                </a>

                                            </td>
                                            <td style="text-align:right;">
                                                <a href="#"
                                                   data-toggle="mymodal"
                                                   ng-click="question();"
                                                   data-target="#mymodal">
                                                    <i class="fa fa-book text-primary"></i>
                                                </a>
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>


                </div>
                
            </div>
        </div>
    </body>




